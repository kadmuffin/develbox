{
    "image": {
        "uri": "opensuse/tumbleweed",
        "on-creation": [
            "zypper -n refresh",
            "zypper -n in dnf libdnf-repo-config-zypp"
        ],
        "on-finish": [],
        "pkg-manager": {
            "operations": {
                "add": "dnf install {args} {-y}",
                "del": "dnf remove {args}",
                "update": "dnf refresh {args}",
                "upgrade": "dnf dup {args}",
                "search": "dnf search {args}"
            },
            "args-modifier": {}
        },
        "env-vars": {}
    },
    "podman": {
        "path": "podman",
        "rootless": true,
        "create-deletion": false,
        "container": {
            "name": "",
            "arguments": [],
            "work-dir": "/code",
            "shell": "/usr/bin/fish",
            "root-user": false,
            "privileged": true,
            "binds": {
                "xorg": true,
                "/dev": true,
                "env-vars": []
            },
            "ports": [],
            "mounts": []
        }
    },
    "commands": {},
    "packages": [],
    "devpackages": [
        "fish"
    ]
}