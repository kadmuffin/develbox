{
    "image": {
        "uri": "opensuse/tumbleweed",
        "on_creation": [
            "zypper -n refresh",
            "zypper -n in dnf libdnf-repo-config-zypp"
        ],
        "on_finish": [],
        "pkgmanager": {
            "operations": {
                "add": "dnf install {args} {-y}",
                "del": "dnf remove {args}",
                "update": "dnf refresh {args}",
                "upgrade": "dnf dup {args}",
                "search": "dnf search {args}",
                "clean": "dnf clean all {args} {-y}"
            },
            "modifiers": {}
        },
        "variables": {}
    },
    "podman": {
        "path": "podman",
        "args": [],
        "rootless": true,
        "auto_delete": false,
        "auto_commit": false,
        "privileged": true
    },
    "container": {
        "name": "",
        "workdir": "/code",
        "shell": "/usr/bin/fish",
        "rootuser": false,
        "binds": {
            "xorg": true,
            "dev": true,
            "variables": []
        },
        "ports": [],
        "mounts": [],
        "shared_folders": {
            "opensuse": [
                "/var/cache/zypp",
                "/var/cache/dnf"
            ]
        }
    },
    "commands": {},
    "packages": [],
    "devpackages": [
        "fish"
    ],
    "userpkgs": {
        "packages": [],
        "devpackages": []
    },
    "experiments": {
        "sockets": false
    }
}