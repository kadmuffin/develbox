{
    "image": {
        "uri": "opensuse/tumbleweed",
        "on-creation": [],
        "on-finish": [],
        "pkg-manager": {
            "operations": {
                "add": "zypper install {args} {--non-interactive}",
                "del": "zypper remove {args}",
                "update": "zypper refresh {args} {--non-interactive}",
                "upgrade": "zypper dup {args} {--non-interactive}",
                "search": "zypper search {args}"
            },
            "args-modifier": {}
        },
        "env-vars": {}
    },
    "podman": {
        "path": "podman",
        "rootless": true,
        "create-deletion": false,
        "container": {
            "name": "",
            "arguments": [],
