{
    "image": {
        "uri": "opensuse/tumbleweed",
        "on-creation": [],
        "on-finish": [],
        "pkg-manager": {
            "operations": {
                "add": "zypper -n install {args}",
                "del": "zypper -n remove {args}",
                "update": "zypper -n refresh {args}",
                "upgrade": "zypper -n dup {args}",
                "search": "zypper -n search {args}",
                "clean": "zypper -n clean --all {args}"
            },
            "args-modifier": {}
        },
        "env-vars": {}
    },
    "podman": {
        "path": "podman",
        "rootless": true,
        "create-deletion": false,
        "container": {
            "name": "",
            "arguments": [],
            "work-dir": "/code",
            "shell": "/usr/bin/fish",
            "root-user": false,
            "privileged": true,
            "binds": {
                "xorg": true,
                "/dev": true,
                "env-vars": []
            },
            "ports": [],
            "mounts": [],
            "experiments": {
                "sockets": false
            },
            "shared-folders": {
                "opensuse": "/var/cache/zypp"
            }
        }
    },
    "commands": {},
    "packages": [],
    "devpackages": [
        "fish"
    ]
}