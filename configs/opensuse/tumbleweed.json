            "shell": "/usr/bin/fish",
            "root-user": false,
            "privileged": true,
            "binds": {
                "xorg": true,
                "/dev": true,
                "env-vars": []
            },
            "ports": [],
            "mounts": []
        }
    },
    "commands": {},
    "packages": [],
    "devpackages": [
        "fish"
    ]
