{
    "image": {
        "uri": "archlinux:latest",
        "on-creation": [],
        "on-finish": [],
        "pkg-manager": {
            "operations": {
                "add": "pacman -S {args}",
                "del": "pacman -R {args}",
                "update": "pacman -Syu {args}",
                "upgrade": "pacman -Syu {args}",
                "search": "pacman -Ss {args}",
                "clean": "pacman -Sc {args}"
            },
            "args-modifier": {}
        },
        "env-vars": {}
    },
    "podman": {
