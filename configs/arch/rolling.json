            "shell": "/usr/bin/fish",
            "root-user": false,
            "privileged": true,
            "binds": {
                "xorg": true,
                "/dev": true,
                "env-vars": []
            },
            "ports": [],
            "mounts": [],
            "experiments": {
                "sockets": false
            },
            "shared-folders": {
                "arch": "/var/cache/pacman/pkg/"
            }
        }
    },
    "commands": {},
    "packages": [],
    "devpackages": [
        "fish"
    ]
