{
    "image": {
        "uri": "debian:bullseye",
        "on-creation": [],
        "on-finish": [],
        "pkg-manager": {
            "operations": {
                "add": "apt-get install {args} {-y}",
                "del": "apt-get remove {args}",
                "update": "apt-get update {args}",
                "upgrade": "apt-get upgrade {args}",
                "search": "apt-get search {args}",
                "clean": "rm -rf /var/cache/apt"
            },
            "args-modifier": {}
        },
        "env-vars": {}
    },
    "podman": {
        "path": "podman",
        "rootless": true,
        "create-deletion": false,
        "container": {
            "name": "",
            "arguments": [],
            "work-dir": "/code",
            "shell": "/usr/bin/fish",
            "root-user": false,
            "privileged": true,
            "binds": {
                "xorg": true,
                "/dev": true,
                "env-vars": []
            },
            "ports": [],
            "mounts": [],
            "experiments": {
                "sockets": false
            },
            "shared-folders": {
                "debian": "/var/cache/apt"
            }
        }
    },
    "commands": {},
    "packages": [],
    "devpackages": [
        "fish"
    ]
